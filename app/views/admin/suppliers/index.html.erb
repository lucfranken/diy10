<p id="notice"><%= notice %></p>

<h1>Admin Suppliers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @suppliers.each do |supplier| %>
      <tr>
        <td><%= supplier.name %></td>
        <td><%= link_to 'Show', admin_supplier_path(supplier) %></td>
        <td><%= link_to 'Edit', edit_admin_supplier_path(supplier) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Supplier', new_admin_supplier_path %>
